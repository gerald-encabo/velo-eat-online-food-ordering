{
    "version": 2,
    "builds":[
        {
            "src": "src/index.tsx",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/robots.txt",
            "status": 204
        },
        {
            "src": "/favicon.ico",
            "status": 204
        },
        { 
            "src": "/(.*)", 
            "dest": "/src/index.tsx"
        }
    ]
}