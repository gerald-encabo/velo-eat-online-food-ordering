{
    "version": 2,
    "builds":[
        {
            "src": ".dist/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/robots.txt",
            "status": 204
        },
        {
            "src": "/favicon.ico",
            "status": 204
        },
        { 
            "src": "/(.*)", 
            "dest": "/dist/index.ts"
        }
    ]
}